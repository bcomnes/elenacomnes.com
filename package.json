{
  "name": "elenacomnes.com",
  "version": "1.0.0",
  "description": "Hiiii",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "run-s test:*",
    "test:deps": "dependency-check ./package.json",
    "test:lint": "standard | snazzy",
    "test:tape": "tape test/* | tap-format-spec",
    "build": "npm run clean && run-p build:*",
    "build:css": "postcss -c postcss.config.js",
    "build:js": "browserify index.js -o dist/bundle.js",
    "build:md": "sitedown . -b dist -l layout.html",
    "build:static": "cpx 'static/**/*.{html,png,jpg}' dist/static",
    "watch": "npm run clean && run-p watch:*",
    "watch:css": "npm run build:css -- --watch",
    "watch:js": "budo index.js:bundle.js --dir dist --live --open",
    "watch:md": "npm run build:md -- -w",
    "watch:static": "npm run build:static -- -w",
    "clean": "rimraf dist && mkdirp dist",
    "start": "npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bcomnes/elenacomnes.com.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bcomnes/elenacomnes.com/issues"
  },
  "homepage": "https://github.com/bcomnes/elenacomnes.com#readme",
  "devDependencies": {
    "@tap-format/spec": "^0.2.0",
    "autoprefixer": "^6.5.2",
    "bel": "^4.5.1",
    "browserify": "^14.3.0",
    "budo": "^10.0.3",
    "cpx": "^1.5.0",
    "dependency-check": "^2.6.0",
    "highlight.js": "^9.8.0",
    "hyperstream": "^1.2.2",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.0.2",
    "postcss": "^5.2.5",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cli": "^3.2.0",
    "postcss-import": "^9.0.0",
    "postcss-reporter": "^3.0.0",
    "postcss-url": "^6.0.4",
    "pump": "^1.0.2",
    "rimraf": "^2.5.4",
    "sitedown": "^3.0.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "style.css": "^1.0.0-alpha-8",
    "tape": "^4.6.2",
    "top-bar.css": "^1.2.0"
  },
  "dependencies": {}
}
